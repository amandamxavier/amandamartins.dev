---
import Card from "../components/Card.astro";

import { getProjects } from "../lib/projects.js";

const projects = await getProjects();
---

{
  projects.map((project) => (
    <Card
      href={`${project.html_url}`}
      title={`${project.name}`}
      body={`${project.description}`}
    />
  ))
}
